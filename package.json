{
  "name": "tiny-qiniu",
  "version": "1.0.0",
  "description": "compress image online with 'tinypng.com' api, then upload to 'qiniu.com'",
  "main": "index.js",
  "scripts": {
    "bundle-ywxx": "browserify src/ywxx-img.js --node --no-bundle-external -t babelify -o dist/ywxx-img.js",
    "bundle-yuxd": "browserify src/yuxd-img.js --node --no-bundle-external -t babelify -o dist/yuxd-img.js",
    "work": "node dist/tinyQn.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yulodl/tiny-qiniu.git"
  },
  "keywords": [
    "compress",
    "tinypng",
    "qiniu"
  ],
  "author": "yulodl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yulodl/tiny-qiniu/issues"
  },
  "homepage": "https://github.com/yulodl/tiny-qiniu#readme",
  "dependencies": {
    "qiniu": "^6.1.13",
    "tinify": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babelify": "^7.3.0"
  }
}
